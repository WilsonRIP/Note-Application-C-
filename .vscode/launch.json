    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug HelloWorld",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/HelloWorld.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [
                {
                    "name": "Path",
                    "value": "${env:Path};C:\\Program Files (x86)\\Windows Kits\\10\\Debuggers\\x64\\"
